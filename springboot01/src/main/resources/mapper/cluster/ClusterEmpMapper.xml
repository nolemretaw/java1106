<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.lanou.mapper.cluster.ClusterEmpMapper">
    <select id="findAllEmp" resultType="com.lanou.bean.cluster.ClusterEmp">
        select * from emp
    </select>

    <insert id="addEmp" parameterType="com.lanou.bean.master.Emp">
        insert into emp(id,name,sex,job,salary,hiredate,deptno)
        values(#{id},#{name},#{sex},#{job},#{salary},#{hiredate},#{deptno})
    </insert>
</mapper>