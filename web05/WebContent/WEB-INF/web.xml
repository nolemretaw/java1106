    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>life</servlet-name>
    <url-pattern>/life</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>first</servlet-name>
    <servlet-class>com.lanou.web.FirstServlet</servlet-class>
  </servlet>
